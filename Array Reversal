#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{int a,b[1000],c=0,i,k;
scanf("%d",&a);
if (a>=1 && a<=1000) 
{
for (i=0;i<a;i++) 
{
scanf("%d",&k); 
if (k>=1 && k<=1000)
{
b[i]=k;
}
}
}
for (i=a-1;i>=0;i--) 
{
printf("%d ",b[i]);
}
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
