#include <stdio.h>
int max_of_four(int a, int b, int c, int d)
{
int e[4]={a,b,c,d},g=e[0],i;
for(i=0;i<4;i++)
{
if (e[i]>=g) 
{
g=e[i];
}
}
return g;
}
int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
